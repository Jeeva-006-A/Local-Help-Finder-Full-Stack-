{
  "version": 2,
  "builds": [
    {
      "src": "LHF_Backend/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "LHF_Front_end/**",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "LHF_Backend/main.py"
    }
  ]
}
